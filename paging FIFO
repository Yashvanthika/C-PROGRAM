#include <stdio.h>
int main() 
{
    int n, frames, pages[100], frame[10], front = 0, faults = 0, hits = 0;
    printf("Enter number of pages and frames: ");
    scanf("%d %d", &n, &frames);
    printf("Enter page reference string: ");
    for (int i = 0; i < n; i++) 
        scanf("%d", &pages[i]);
    for (int i = 0; i < frames; i++) 
        frame[i] = -1;
    for (int i = 0; i < n; i++) 
	{
        int found = 0;
        for (int j = 0; j < frames; j++) 
		{
            if (frame[j] == pages[i]) 
			{
                found = 1; 
                hits++;
                break;
            }
        }
        if (!found) 
		{  
            frame[front] = pages[i];  
            front = (front + 1) % frames;  
            faults++;
        }
        printf("Frames: ");
        for (int j = 0; j < frames; j++) 
		{
            if (frame[j] == -1)
                printf("- ");
            else
                printf("%d ", frame[j]);
        }
        printf("\n");
    }
    printf("Total page faults: %d\n", faults);
    printf("Total page hits: %d\n", hits);
    return 0;
}
